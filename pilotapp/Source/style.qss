QMainWindow {
    background-color: #FFFFFF;
}

QLabel {
    color: black;
    font: 'Inter';
}

QPushButton {
    border: 2px solid #c7c7c7;
    border-radius: 8px;
    padding: 1px 4px;
    min-width: 30px;
    min-height: 16px;
    outline: 0;
    color: black;
    font: 'Inter';
}

QPushButton:pressed {
    border-width: 2px;      
    background-color: #c7c7c7;
    border-color: #c7c7c7;
    outline: 0;
    color: black;
}

QPushButton#iconPushButton {
    border: 0;
    border-radius: 0px;
    padding: 0px 0px;
    outline: 0;
}

QPushButton#iconPushButton:pressed {
    border-width: 0px;
    background-color: #000000;
    border-color: #000000;
    border: 0;
    border-radius: 0px;
    outline: 0;
}


